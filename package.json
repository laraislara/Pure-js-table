    "start": "parcel src/index.html -p 3000 --out-dir build",
    "build": "yarn lint && yarn clean && yarn build:parcel",
    "lint": "yarn lint:js",
    "lint:js:fix": "eslint ./app/**/*.js --fix",
    "lint:js": "eslint --ignore-path .gitignore -- ./src",
